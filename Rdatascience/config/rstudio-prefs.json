{
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true
    },
    "auto_expand_error_tracebacks": true,
    "show_last_dot_value": true,
    "continue_comments_on_newline": true,
    "highlight_selected_line": true,
    "show_invisibles": true,
    "syntax_color_console": true,
    "highlight_r_function_calls": true,
    "console_line_length_limit": 2000,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true,
    "show_help_tooltip_on_idle": true,
    "tab_multiline_completion": true,
    "code_completion_characters": 2,
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "warn_if_no_such_variable_in_scope": true,
    "warn_variable_defined_but_not_used": true,
    "style_diagnostics": true,
    "save_files_before_build": true,
    "cran_mirror": {
        "name": "Global (CDN)",
        "host": "RStudio",
        "url": "https://cran.rstudio.com/",
        "country": "us",
        "ok": 1,
        "secondary": ""
    },
    "use_tinytex": true,
    "posix_terminal_shell": "custom",
    "custom_shell_command": "~/miniconda/bin/radian",
    "terminal_close_behavior": "never",
    "show_hidden_files": true,
    "initial_working_directory": "~/development/",
    "rainbow_parentheses": true,
    "line_ending_conversion": "posix",
    "visual_markdown_editing_is_default": true,
    "editor_theme": "Dracula",
    "show_doc_outline_rmd": true,
    "insert_native_pipe_operator": true,
    "jobs_tab_visibility": "shown",
    "auto_detect_indentation": true,
    "default_latex_program": "XeLaTeX",
    "remove_history_duplicates": true,
    "soft_wrap_r_files": true,
    "default_encoding": "UTF-8",
    "visual_markdown_editing_show_margin": true,
    "visual_markdown_code_editor_line_numbers": true,
    "visual_markdown_editing_wrap_at_column": 80,
    "visual_markdown_editing_wrap": "column",
    "enable_cloud_publish_ui": false,
    "show_publish_diagnostics": true,
    "python_type": "conda",
    "python_version": "3.12.0",
    "python_path": "~/miniconda/bin/python3.12"
}